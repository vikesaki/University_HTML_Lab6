            .block-03
                .table-2
                    .title-1-2
                        p Sorting:
                    .left-edge-2
                        p First level
                        p Second level
                        p Third level
                    .center-edge-2
                        p
                            +my-option
                        p
                            +my-option
                        p
                            +my-option
                    .right-edge-2
                        p.no-ind descending?
                            input(type='checkbox', name='ack1', value='ack1')
                        p.no-ind descending?
                            input(type='checkbox', name='ack1', value='ack1')
                        p.no-ind descending?
                            input(type='checkbox', name='ack1', value='ack1')
