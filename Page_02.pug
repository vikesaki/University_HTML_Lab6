extends setting

mixin my-option
    select
        option(value='0') No
        option(value='1') Name
        option(value='2') Location
        option(value='3') Province
        option(value='4') Visitor
        option(value='5') Price

block title
    title List of attractions

mixin table-create(a, b, c, d, e)
    tr
        td=a
        td=b
        td=c
        td=d
        td=e

block content
    details
        summary Display Settings Block:
        .main-block
            .block-01
                .table
                    .title-1-1
                        p Chart:
                    .left-edge-1
                        p.title-1 Option
                        p Visitor
                            input(type='radio', name='ox', value='жанр')
                        p Location
                            input(type='radio', name='ox', value='жанр')
                        p Province
                            input(type='radio', name='ox', value='жанр')
                        p Price
                            input(type='radio', name='ox', value='жанр')
                    .right-edge-1
                        p.title-1 Result
                        p Maximum
                            input(type='checkbox', name='result', value='макс')
                        p Minimum
                            input(type='checkbox', name='result', value='мин')
                    button Build
            .block-02
                .table-2-2
                    .table
                        .title-1-3
